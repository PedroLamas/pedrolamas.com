    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  },
  "repository": {
    "type": "git",
