    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=12.0",
    "npm": "npm is not supported",
    "yarn": ">=1.22"
  },
  "repository": {
    "type": "git",
