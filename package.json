    "gatsby-remark-smartypants": "^2.1.8",
    "gatsby-source-filesystem": "^2.1.25",
    "gatsby-transformer-sharp": "^2.2.15",
    "gatsby-transformer-yaml": "^2.2.9",
    "github-slugger": "^1.2.1",
    "prism-react-renderer": "^0.1.7",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
