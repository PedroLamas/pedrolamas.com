    "@fortawesome/react-fontawesome": "^0.1.11",
    "@mdx-js/mdx": "^1.6.18",
    "@mdx-js/react": "^1.6.18",
    "@mdx-js/tag": "^0.20.3",
    "disqus-react": "^1.0.10",
    "gatsby": "^2.24.80",
    "gatsby-cli": "^2.12.109",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-feed-generator": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.3.18",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-mdx": "^1.2.46",
    "gatsby-plugin-netlify": "^2.3.20",
    "gatsby-plugin-offline": "^3.2.31",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sass": "^2.3.16",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-plugin-twitter": "^2.3.12",
    "gatsby-plugin-typescript": "^2.4.21",
    "gatsby-remark-acronyms": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-images": "^3.3.35",
